{
  "name": "mock-fh5",
  "version": "0.0.380",
  "description": "prototype - simple REST api server with Forza Horizon 5 data...",
  "author": "eswat2",
  "repository": "eswat2/mock-fh5",
  "license": "MIT",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "chance": "1.1.13",
    "fs": "0.0.1-security",
    "nanoid": "5.1.6",
    "papaparse": "5.5.3",
    "path": "0.12.7",
    "vin-generator": "1.0.1"
  },
  "devDependencies": {
    "bumpp": "10.4.0",
    "chai": "6.2.2",
    "cspell": "9.6.3",
    "env-cmd": "11.0.0",
    "mocha": "11.7.5",
    "only-allow": "1.2.2",
    "prettier": "3.8.1",
    "vercel": "50.9.6",
    "yoctocolors": "2.1.2"
  },
  "scripts": {
    "bump": "bumpp patch --yes",
    "deploy-app": "vc --prod",
    "release": "pnpm bump && pnpm build && pnpm test && pnpm deploy-app",
    "preinstall": "npx only-allow pnpm",
    "sample": "node sample.mjs",
    "dev": "env-cmd -x -- vc dev --listen 8082 --token \\$API_TOKEN",
    "cc": "prettier --check api utils tests",
    "format": "prettier --write api utils tests",
    "spell": "cspell './**/*.js'",
    "test": "mocha ./tests/*.js",
    "build": "yarn format",
    "open": "open https://mock-fh5.vercel.app",
    "cars": "open https://mock-fh5.vercel.app/api/cars",
    "solution": "open https://mock-fh5.vercel.app/api/solution"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  }
}
