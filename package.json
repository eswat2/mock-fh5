{
  "name": "mock-fh5",
  "version": "0.0.240",
  "description": "prototype - simple REST api server with Forza Horizon 5 data...",
  "author": "eswat2",
  "repository": "eswat2/mock-fh5",
  "license": "MIT",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "dependencies": {
    "chance": "1.1.13",
    "fs": "0.0.1-security",
    "nanoid": "5.1.5",
    "papaparse": "5.5.3",
    "path": "0.12.7",
    "vin-generator": "1.0.1"
  },
  "devDependencies": {
    "chai": "5.2.0",
    "cspell": "9.0.2",
    "env-cmd": "10.1.0",
    "mocha": "11.6.0",
    "prettier": "3.5.3",
    "vercel": "43.1.0"
  },
  "scripts": {
    "sample": "node sample.mjs",
    "dev": "env-cmd -x -- vc dev --listen 8082 --token \\$API_TOKEN",
    "cc": "prettier --check api utils tests",
    "format": "prettier --write api utils tests",
    "spell": "cspell './**/*.js'",
    "test": "mocha ./tests/*.js",
    "build": "yarn format",
    "predeploy": "yarn test; yarn version --patch",
    "deploy": "vc --prod",
    "open": "open https://mock-fh5.vercel.app",
    "cars": "open https://mock-fh5.vercel.app/api/cars",
    "solution": "open https://mock-fh5.vercel.app/api/solution"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  }
}
